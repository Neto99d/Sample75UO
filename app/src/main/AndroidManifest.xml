<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.neto.sample75uo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sample75UO"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.options.PteFeuActivity" android:label="Presidentes de la FEU"/>
        <activity android:name=".ui.options.CampannaActivity" android:label="Campañas"/>
        <activity android:name=".ui.options.HonorCausaActivity" android:label="Honoris Causa"/>
        <activity android:name=".ui.options.ProfeMeritoActivity" android:label="Profesores Eméritos"/>
        <activity android:name=".ui.options.EstadisticasActivity" android:label="Estadísticas"/>
        <activity android:name=".ui.options.IdentidadActivity" android:label="Identidad"/>
        <activity android:name=".ui.options.RectoresActivity" android:label="Rectores"/>
        <activity android:name=".ui.options.EfemeridesActivity" android:label="Efemérides"/>
        <activity android:name=".ui.options.FullImageScreenActivity" android:label="Imagen Ampliada"/>
        <activity
            android:name=".ui.options.PatrimonioActivity"
            android:label="Patrimonio" />
        <activity
            android:name=".ui.options.NewsActivity"
            android:label="La Tablilla" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Sample75UO.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>